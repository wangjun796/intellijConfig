<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Class...">
        <Item>
          <IdeaActionID>GotoClass</IdeaActionID>
          <ShortCut>'Ctrl+N'</ShortCut>
          <Description>Class...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Navigation Bar">
        <Item>
          <IdeaActionID>ShowNavBar</IdeaActionID>
          <ShortCut>'Alt+Home'</ShortCut>
          <Description>Jump to Navigation Bar</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Coverage Data...">
        <Item>
          <IdeaActionID>SwitchCoverage</IdeaActionID>
          <ShortCut>'Ctrl+Alt+F6'</ShortCut>
          <Description>Show Coverage Data...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Type Declaration">
        <Item>
          <IdeaActionID>GotoTypeDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+Shift+B' or 'Ctrl+Shift+Button1 Click'</ShortCut>
          <Description>Type Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Vim Emulator">
        <Item>
          <IdeaActionID>VimPluginToggle</IdeaActionID>
          <ShortCut>'Ctrl+Alt+V'</ShortCut>
          <Description>Vim Emulator</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>